function Kanyaku(zxcbe, Gyaku){
    var zxc;   
    if(Gyaku==true){
        
       zxc=setarrayy(zxcbe.length,2*zxcbe[0].length)
       for(var plm=0; plm<zxc.length; plm++){
           for(var ham=0; ham<zxc[0].length; ham++){
                if(ham>=zxcbe.length){
                   if(plm==ham-zxcbe.length){
                     zxc[plm][ham]=1.0;
                 }else{
                   zxc[plm][ham]=0.0;
                 }
               }else{
                   zxc[plm][ham]=zxcbe[plm][ham];
               }
           }
       }
    }else{
        zxc=zxcbe;
    }

       var y_size=zxc.length;
       var x_size=zxc[0].length;
       var pivot_pos=-1;
       var pivot_elm=-1.0;

      zxc=sortt(zxc);//
       
      for(var y=0; y<y_size; y++){
        for(var x=pivot_pos+1; x<x_size; x++ ){
          if(zxc[y][x]!=0){
            pivot_pos=x;
            break;
          }
        }

        pivot_elm=zxc[y][pivot_pos];

        if(pivot_elm==0){
          break;
        }

        for(var x=0; x<x_size; x++){
          zxc[y][x] /=pivot_elm;
        }

        for(var y_lower=y+1; y_lower<y_size; y_lower++){
          var ccc=zxc[y_lower][pivot_pos];
          for(var x=0; x<x_size; x++){
            zxc[y_lower][x]-=ccc*zxc[y][x];
          }
        }
      }

      zxc=sortt(zxc);
      pivot_pos=-1;
      for(var y=0; y<y_size; y++){
       for(var x=pivot_pos+1; x<x_size; x++ ){
         if(zxc[y][x]!=0){
           pivot_pos=x;
           break;
         }
       }

       pivot_elm=zxc[y][pivot_pos];

       if(pivot_elm==0){
         break;
       }

       for(var x=0; x<x_size; x++){
         zxc[y][x] /=pivot_elm;
       }

       for(var y_upper=0; y_upper<y; y_upper++){
         var ccc=zxc[y_upper][pivot_pos];
         for(var x=0; x<x_size; x++){
           zxc[y_upper][x]-=ccc*zxc[y][x];
         }
       }
     }
    
     
     if(Gyaku==true){
       
       for(var plm=0; plm<zxc.length; plm++){
           for(var ham=0; ham<zxc[0].length; ham++){
                if(ham>=zxcbe.length){
                  zxcbe[plm][ham-zxcbe.length]=zxc[plm][ham];
               }
           }
       }
   }

     

       return zxcbe;


}
