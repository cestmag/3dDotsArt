  function sortt(abcdefu){
   
    var y_size=abcdefu.length;
    var x_size=abcdefu[0].length;
   
                                            
    var addzxc=setarrayy(y_size, x_size+1);
    
    
   
    for(var bad=0; bad<y_size; bad++){
       for(var us=0; us<x_size+1; us++){
         if(us<x_size){
          
           addzxc[bad][us]=abcdefu[bad][us];
         }else{
          addzxc[bad][us]=forKanyaku(abcdefu[bad]);
         }
       }
    }

    addzxc.sort(function(koan,fuck){
        return koan[x_size]-fuck[x_size];
    })

    
    
    for(var bad=0; bad<y_size; bad++){
      for(var us=0; us<x_size+1; us++){
        if(us<x_size){
          abcdefu[bad][us]=addzxc[bad][us];
        }
      }
   }
 
    return abcdefu;
} 

